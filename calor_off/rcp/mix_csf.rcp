\START    CSF_RCP
\SIZE         76        74
!----------------------------------------------------------------------
!    Name:       CSF.RCP
!    Package:
!    Library:
!    Purpose:    Control file for CSFMAKE. Contains conversion factors
!                A from ADC counts to total energy and weights for
!                each calorimeter module. This set to made to produe identical
!                results as the CAD_GAIN function produces for mixture 
!                D0GEANT.
!    Version     MIX_1.1 
!    Created:    2-MAR-1992   Chip Stewart
!----------------------------------------------------------------------
 DATA_SOURCE 'MIX'
!----------------------------------------------------------------------
!   Factors to convert from ADC counts to total energy
!----------------------------------------------------------------------
\ARRAY A
!   CCEM        ECEM        CCMG        ICD         ECMG
!   CCFH        ECIH        ECMH        CCCH        ECOH
    10*0.003205128
\END
!----------------------------------------------------------------------
!CCEM
!----------------------------------------------------------------------
\ARRAY W_CCEM
 7*1.0     
\END
CORRECT_CCEM F
!----------------------------------------------------------------------
! ECEM
!----------------------------------------------------------------------
\ARRAY W_ECEM
 7*1.33      
\END
CORRECT_ECEM  F
!----------------------------------------------------------------------
! CCMG
!----------------------------------------------------------------------
\ARRAY W_CCMG
 5*0.0624
\END
CORRECT_CCMG  F
!----------------------------------------------------------------------
! ICD
!----------------------------------------------------------------------
\ARRAY W_ICD
 6*0.1248
\END
CORRECT_ICD F
!----------------------------------------------------------------------
! ECMG
!----------------------------------------------------------------------
\ARRAY W_ECMG
 6*0.0624
\END
CORRECT_ECMG  F
!----------------------------------------------------------------------
! CCFH
!----------------------------------------------------------------------
\ARRAY W_CCFH
 3*1.87
\END
CORRECT_CCFH  F
!----------------------------------------------------------------------
! ECIH
!----------------------------------------------------------------------
\ARRAY W_ECIH
 4*1.87 7.53
\END
CORRECT_ECIH  F
!----------------------------------------------------------------------
! ECMH
!----------------------------------------------------------------------
\ARRAY W_ECMH
 4*1.87  7.53
\END
CORRECT_ECMH F
!----------------------------------------------------------------------
! CCCH
!----------------------------------------------------------------------
\ARRAY W_CCCH
 7.59
\END
CORRECT_CCCH  F
!----------------------------------------------------------------------
! ECOH
!----------------------------------------------------------------------
\ARRAY W_ECOH
 3*7.53
\END
CORRECT_ECOH  F
\STOP    !   CSF_RCP
