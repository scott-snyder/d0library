\END
CORRECT_ECIH  F
!----------------------------------------------------------------------
! ECMH      LYR 11-15
!----------------------------------------------------------------------
\ARRAY W_ECMH
 1.487   1.289   1.294   1.294   5.438
\END
CORRECT_ECMH F
!----------------------------------------------------------------------
