\START     FDC_RCP        ! Name of file
\SIZE         40        40

!------------------------------------------------------------------------------
!   Name:       FDC.RCP
!   Purpose:    Used in FDC_EXM package, FDC_CALIB package and personal code.
!
!------------------------------------------------------------------------------
!   HBOOK DIRECTORY structure - used by FDBHST_EXM, FDFHST_EXM
!------------------------------------------------------------------------------
\ARRAY HBOOK_FDC
      'FDC'
\END
\ARRAY HBOOK_CD
      'CD'
\END
\ARRAY FDC_XSECT
      'FDC'
\END
\ARRAY FDC_XSW
      'FDC'
\END
!------------------------------------------------------------------------------
 FDC_ONLY           TRUE    ! TRUE enables optional histograms menu item
!                           ! and booking of more histograms
!------------------------------------------------------------------------------
! Used in FDC_XSECT package:
 FB_SHORT_HISTS     TRUE    ! TRUE to only book four velocity histograms
!                           ! using cross sector segments
 FB_MXCHSQ_XSECT        50. ! Maxmimum Chisq allowed for X-sector segment fit
 FB_SLOPE_CUT         0.80  ! Maxmimum segment slope allowed for
!                           !   X-sector and X-sw segments.
!------------------------------------------------------------------------------
! Used in FDC_XSW package:
 FB_SHORT_HISTS_SW  TRUE    ! TRUE to only book four velocity histograms
!                           ! using cross sector segments
 FB_MXCHSQ_XSW          50. ! Maxmimum Chisq allowed for X-sector segment fit
!FB_SLOPE_CUT         0.80  !   same as above
!------------------------------------------------------------------------------
\STOP
