Local_Mem_Ctrl_Reg_Value        .set    1DCC4000h       ; Value to use for
                                                        ; Local Memory
                                                        ; Interface Control
                                                        ; Register

Rack_1_EM_ComPort_Input .set        0                   ; There is no Rack
