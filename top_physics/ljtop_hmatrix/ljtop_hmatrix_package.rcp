 \END
 \ARRAY HBOOK_DIRECTORY
      'HMATRIX'
 \END
NTUPLE_PRIMARY_ALLOCATION 20000 !PRIMARY ALLOCATION FOR NTUPLE
NTUPLE_ID   1000   !ID of QUAN ntuple
\ARRAY NTUPLE_FILE
 'HMATRIX_NTUPLE'
\END
