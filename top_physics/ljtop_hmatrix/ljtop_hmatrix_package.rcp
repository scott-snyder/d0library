!
 ACCUMULATE_HMATRIX F        !IF FALSE, WILL USE H MATRIX ALREADY STORED.
 DIAGONALIZE  TRUE           !IF TRUE WILL DIAGONALIZE QUANTITIES IN USAGE MODE
!------------------------------------------------------------------------------
!   RZ FILE SETUP
!------------------------------------------------------------------------------
!
NEW_RZ F     !will open new RZ file
RZ_LOG_LEVEL 1     !LOG LEVEL OF RZ
LIST_RZ_DIRECTORY  .true.       !Will list RZ directory
\ARRAY RZ_FILE_NAME
    'HMATRIX_SAVE'
\END
\ARRAY RZ_FILE_MODE
 'U'   !Mode for opening old data base
\END
\ARRAY TOP_DIRECTORY
 'HMATRIX'
\END
\ARRAY SUB_DIRECTORY
 'TTB140'
\END
\ARRAY USE_HMATRIX      !THE FOLLOWING H MATRICES WILL BE USED
 'WJETS_DATA'
 'VECBOS3'
 'VECBOS4'
 'QCD_FAKE'
 'TTB140'
 'TTB160'
 'TTB180'
\END
\ARRAY KEY_DESCR
'I'
\END
\ARRAY CHTAG
  'HMATRIX'
\END
!
PRIMARY_ALLOCATION 62000
RECORD_LENGTH     1024
NUMBER_OF_WARNINGS      99999   !Error messages
!
