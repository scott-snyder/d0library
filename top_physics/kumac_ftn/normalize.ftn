      SUBROUTINE NORMALIZE(XABS,BCK,SIG,XLL,XHH,FACT)
      REAL XABS(100),BCK(100),SIG(100)
      REAL    FACT,XLL,XJJ,SIGS,BCKS
      SIGS= 0.
      BCKS=0.
      PRINT *, XABS(1),BCK(99),SIG(1)
      DO 10 I = 1,50
        IF(XABS(I).GE.XLL.AND.XABS(I).LE.XHH)THEN
          SIGS= SIGS + SIG(I)
          BCKS = BCKS + BCK(I)
        ENDIF
   10 CONTINUE
      PRINT *, 'SIGS,BCKS,FACT ',SIGS,BCKS,FACT
      FACT = SIGS/BCKS
      END

