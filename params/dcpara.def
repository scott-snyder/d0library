C
C         Parameters for digitization
C
C         Readout parameters
      INTEGER NCRATE,OADC,NADC,NTIMSL,NCHN,NBLS,NEF,NTWRS
      PARAMETER (NCRATE=1)
      PARAMETER (NADC =6)
      PARAMETER (OADC =8)
      PARAMETER (NCHN=24)
      PARAMETER (NTIMSL=16)
      PARAMETER (NBLS=8)
      PARAMETER (NEF=4)
      PARAMETER (NTWRS=4)
C         Cable parameters
      INTEGER NDEP,NPHI,NETA,NSECT,NQUAD
      PARAMETER (NDEP=12)
      PARAMETER (NPHI=2)
      PARAMETER (NETA=12)
      PARAMETER (NSECT=4)
      PARAMETER (NQUAD=2)
C         Module parameters
      INTEGER MDEP,MPHI,META,MSECT,MQUAD
      PARAMETER (MDEP=7)
      PARAMETER (MPHI=2)
      PARAMETER (META=12)
      PARAMETER (MSECT=5)
      PARAMETER (MQUAD=2)
C        Physics parameters
      INTEGER NPHIC,NETAC,NLYRC
      PARAMETER (NPHIC=6)
      PARAMETER (NETAC=24)
      PARAMETER (NLYRC=14)
