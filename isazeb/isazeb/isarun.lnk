$ SET VERIFY
$ !
$ ! make ISARUN.EXE to generate tracks in ISAJET Zebra format
$ !
$ SWIT = ""
$ DEB  = ""
$ IF P1 .NES. "DEBUG" THEN GOTO NODEB
$   SWIT = "/DEBUG"
$   DEB =  "DEB_"
$ NODEB:
$ !
$  LINK'SWIT'/NOMAP/EXE='DEB'ISARUN -
D0$ISAZEB:'DEB'ISAZEB/INCLUDE=(ISARUN,ISAWBG,ISAWEV,ISAWND,ZABEND)/L,-
D0$ISAJET:ISAJET/INCLUDE=(ALDATA)/L,-
D0$GENERAL:'DEB'GENERAL/INCLUDE=DBANK/L,-
'CERNP'
$ !
$ SET NOVERIFY
