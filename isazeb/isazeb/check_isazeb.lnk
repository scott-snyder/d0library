$ SET VERIFY
$ !
$ ! make CHK_ISAZEB.EXE to check an ISAJET Zebra output file
$ !
$ SWIT = ""
$ DEB  = ""
$ IF P1 .NES. "DEBUG" THEN GOTO NODEB
$   SWIT = "/DEBUG"
$   DEB =  "DEB_"
$ NODEB:
$ !
$ LINK'SWIT'/NOMAP/EXEC=CHECK_ISAZEB -
D0$ISAZEB:'DEB'ISAZEB/INCLUDE=(CHKISZ)/L,-
D0$GENERAL:'DEB'GENERAL/INCLUDE=DBANK/L,-
D0$ISAJET:ISAJET/INCLUDE=(ALDATA)/L,-
'CERNP'
$ SET NOVERIFY
