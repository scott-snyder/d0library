$ SET VERIFY
$ !
$ ! make TRACKS.EXE to generate tracks in ISAJET Zebra format
$ !
$ SWIT = ""
$ DEB  = ""
$ IF P1 .NES. "DEBUG" THEN GOTO NODEB
$   SWIT = "/DEBUG"
$   DEB =  "DEB_"
$ NODEB:
$ !
$ LINK'SWIT'/NOMAP/EXEC='DEB'TRACKS -
D0$ISAZEB:'DEB'ISAZEB/INCLUDE=(TRACKS,ISAWBG,ISAWEV,ISAWND)/L,-
D0$ISAJET:ISAJET/INCLUDE=(ALDATA),ISAJET/L,-
D0$GENERAL:'DEB'GENERAL/INCLUDE=DBANK/L,-
'CERNP'
$ SET NOVERIFY
