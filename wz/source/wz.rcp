!------------------------------------------------------------------------------
 ETACUT        1.7
 PTCUT         5.0
!------------------------------------------------------------------------------
! Parameters for W-> mu nu analysis
!------------------------------------------------------------------------------
 ANALYZE_W_MUNU        .TRUE.  ! Analyzes W -> mu nu
 W_PTMIN1     15.0    ! minimum Pt of highest Pt muon
 W_PTMAX2   999999.0    ! maximum Pt of second highest Pt muon
 W_MISS_ET    15.0    ! minimum missing Et in the event
 W_MASS_LO     0.0    ! minimum value for W transverse mass
 W_MASS_HI  999999.0    ! maximum value for W transverse mass
 W_ISOMAX        5.0    ! maximum value for isolation (2nn)
 W_ISOMIN       -2.0    ! minimum value for isolation (2nn)
 W_IMPCXY       15.0    ! maximum value for non bend view impact parameter
 W_DO_GFIT_CUTS  .FALSE.  ! ask that global fit was succesful
 W_CHISQ_GFIT     50.     ! max value for global fit chi squared
 W_CHTOP 'WZMUNT'
!------------------------------------------------------------------------------
! Parameters for Z-> mu mu analysis
!------------------------------------------------------------------------------
 ANALYZE_Z_MUMU        .TRUE.  ! Analyzes Z -> mu mu
 Z_PTMIN1    15.0    ! minimum Pt of highest Pt muon
 Z_PTMIN2    10.0     ! minimum Pt of second highest Pt muon
 Z_ISOMAX     5.0    ! maximum value for isolation (2nn)
 Z_ISOMIN    -2.0    ! minimum value for isolation (2nn)
 Z_IMPCXY    15.0    ! maximum value for non bend view impact parameter
 Z_MASS_LO     0.0    ! minimum value for Z transverse mass
 Z_MASS_HI  999999.0    ! maximum value for Z transverse mass
 DO_MU_SIGN   .FALSE.  ! ask that the two muons have different charge
 DO_TIGHT_Z_CUT  .FALSE.   ! ask that both muons pass "good" muon selection
 DO_LOOSE_Z_CUT   .TRUE.  ! ask that at least one muon passes "good" muon selection
 OCTANT_CUT    .TRUE.  ! ask that both muons do not cross octants
 IFW4CUT        1      ! maximum value for IFW4 allowed for both muons
 NO_OPP_TRACK    .TRUE. ! reject events where Dphi<DELTA_PHI and Dtheta<DELTA_THE between both tracks in GOOD_Z selection.
 DELTA_PHI       160.0     ! angle in degrees
 DELTA_THE       170.0     ! angle in degrees
 Z_DO_GFIT_CUTS  .FALSE.  ! ask that global fit was succesful for at least one muon
 Z_CHISQ_GFIT     50.     ! max value for global fit chi squared
 DO_DPHI_CUT     .FALSE.   ! cut on the minimum opening angle between the two muons
 DPHICUT         30.      ! minimum value for the opening angle between the muons (in degrees)
 Z_CHTOP 'ZMMUNT'
