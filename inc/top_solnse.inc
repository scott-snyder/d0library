C----------------------------------------------------------------------
C-
C-   Created  26-MAR-1993   Rajendran Raja
C-   common block for exact ellipse solutions for top masses
C-
C----------------------------------------------------------------------
      INTEGER MAXT  
      PARAMETER( MAXT = 200 )
      COMMON /TOP_SOLNSE/ TOP_MASS(MAXT),TOP_CROSS(MAXT),
     &  TOP_QQB(MAXT),TOP_GG(MAXT),LIKELY(MAXT,2,2),
     &  TMASS_ON(2),RR_MASS(2),RR_LIKELY(2),RR_LO(2),RR_HI(2),
     &  DG_MASS(2),DG_LIKELY(2),DG_LO(2),DG_HI(2),
     &  RWEIGHT(4,MAXT),WEIGHTD(4,MAXT),NEUT1(4,4,MAXT),WB1(4,4,MAXT),
     &  T1(4,4,MAXT),NEUT2(4,4,MAXT),WB2(4,4,MAXT),T2(4,4,MAXT),
     &  TMASS_LO,TMASS_HI,DELMASS,TMASSE,TAU_MIN,
     &  SHATD,TMIN,TMAX,
     &  NSOLS(MAXT),NTOPS,ICONFG,ICMB,NCNFGE,COMBNUM,QUARK
C
      DOUBLE PRECISION    TOP_MASS,RWEIGHT,WEIGHTD,NEUT1,WB1,T1
      DOUBLE PRECISION    TOP_CROSS,TOP_QQB,TOP_GG,LIKELY
      DOUBLE PRECISION    TMASS_ON,RR_MASS,RR_LIKELY,RR_LO,RR_HI
      DOUBLE PRECISION    DG_MASS,DG_LIKELY,DG_LO,DG_HI
      DOUBLE PRECISION    NEUT2,WB2,T2,TAU_MIN,SHATD,TMIN,TMAX
      DOUBLE PRECISION    TMASS_LO,TMASS_HI,DELMASS,TMASSE
C
      INTEGER NSOLS,NTOPS,ICONFG,ICMB,NCNFGE,COMBNUM
      LOGICAL QUARK
C TOP_MASS contains the top masses generated
C TOP_CROSS contains the top cross section
C TOP_QQB = QQBAR CONTRIBUTION TO TOP CROSS SECTION
C TOP_GG = GLUON FUSION CONTRIBUTION TO TOP_CROSS SECTION
C LIKELY(IM,ICMB,IRR) = LIKELIHOOD FOR EACH TOP MASS FOR MASS
C INDEX IM, COMBINATION INDEX ICMB (=1, HIGH ET LEPTON WITH 1ST JET, = 2
C WITH SECOND JET), IRR=1 RR LIKELIHOOD, =2 DG LIKELIHOOD.
C
C TMASS_ON = MASS AT WHICH 1ST SOLUTION TURNS ON FOR ICMB=1,2
C RR_MASS = MASS AT MAXIMUM LIKELIHOOD USING RR METHOD FOR ICMB=1,2
C RR_LIKELY = MAXIMUM LIKELIHOOD VALUE FOR RR METHOD ICMB=1,2
C RR_LO     = 90% CL LOWER LIMIT FOR RR METHOD ICMB=1,2
C RR_HI     = 90% UPPER LIMIT FOR RR METHOD ICMB=1,2
C
C DG_MASS = MASS AT MAXIMUM LIKELIHOOD USING DG METHOD FOR ICMB=1,2
C DG_LIKELY = MAXIMUM LIKELIHOOD VALUE FOR DG METHOD ICMB=1,2
C DG_LO     = 90% CL LOWER LIMIT FOR DG METHOD ICMB=1,2
C DG_HI     = 90% UPPER LIMIT FOR DG METHOD ICMB=1,2
C
C RWEIGHT Proper weight for solution containing Jacobian etc
C WEIGHTD Dalitz Weight
C Neut1,WB1,T1 = Neutrino,W and Top for 1st  top
C Neut2,WB2,T2 = Neutrino,W and Top for 2nd  top
C NSOLS = Number of solutions
C NTOPS = Number of Top masses 
C TMASS_LO,TMASS_HI,DELMASS = HI AND LO LIMITS OF TOP MASS
C + INCREMENT
C TMASSE = CURRENT VALUE OF TOPMASS
C TAU_MIN = minimum value of tau
C SHATD = S HAT FOR PROCESS
C TMIN,TMAX = minimum and maximum values of T
C QUARK = .TRUE. WILL CALCULATE QUARK PART OF TTBAR CROSS SECTION
C ELSE WILL CALCULATE GLUON PART
C ICONFG = CURRENT CONFIG NUMBER 
C ICMB = CURRENT COMBINATION NUMBER =1 ORIG. =2 SWAPPED LEPTONS
C NCNFGE = NUMBER OF EXACT CONFIGURATIONS TO GENERATE
C COMBNUM = Combination number of present jets
