      COMMON /EVENT_QUAN_2C/ RUNC,EVENTC,LEPTON1(7),LEPTON_TYPE,
     &  IDUM, NEUTRINO(2),BJET_LEPTON(7),BJET_HADRON(7),
     &  JET1_W(7),JET2_W(7),ISR_LIKE,FSR_LIKE,
     &  ITOT_EV,IACC_EV,IFIT_EV,COMBNUM
C
      INTEGER RUNC,EVENTC,IDUM
      INTEGER ITOT_EV,IACC_EV,IFIT_EV
      DOUBLE PRECISION LEPTON1
      DOUBLE PRECISION BJET_LEPTON,BJET_HADRON
      DOUBLE PRECISION JET1_W,JET2_W,ISR_LIKE,FSR_LIKE
      DOUBLE PRECISION NEUTRINO
      INTEGER LEPTON_TYPE
      INTEGER COMBNUM
