      INTEGER MXDN1,MXDA1   
      PARAMETER (MXDN1=120,MXDA1=20000) 
      COMMON/DHSDAT/MXDNAM,NDNAM,DNAPNT(2,MXDN1),DNATYP(MXDN1), 
     1  DNAFLG(MXDN1),MXDHAT,NDHAT,KHAD(MXDA1), 
     2  DNANAM(MXDN1),DHACS 
      CHARACTER DNANAM*16,DHACS*800 
      INTEGER NDNAM,DNAPNT,DNATYP,DNAFLG,NDHAT,KHAD 
      REAL DHAL(MXDA1)  
      LOGICAL LAHD(MXDA1)   
      EQUIVALENCE (KHAD,DHAL,LAHD)  
C   
C  **************  BEGIN DESCRIPTION OF /DHSDAT/  ************  
C   
C  MXDNAM      :  MAXIMUM NUMBER OF DATA BLOCKS.    
C  NDNAM       :  CURRENT NUMBER OF DATA BLOCKS.    
C  DNAPNT(I,K) :  POINTER TO DATA FOR DATA BLOCK K. 
C  DNATYP(K)   :  DATA TYPE FOR BLOCK K.  = 1 FOR INTEGER,  
C              :  = 2 FOR REAL, = 3 FOR LOGICAL, = 4 CHARACTER. 
C  DNAFLG(K)   :  STATUS FLAG. = -1 IMMEDIATE DELETE, = 0   
C              :  DELETE AFTER EVENT PROCESSING, = 1 KEEP.  
C  MXDHAT      :  MAXIMUM NUMBER OF DATA ITEMS. 
C  NDHAT       :  CURRENT NUMBER OF DATA ENTRIES.   
C  KHAD(L)     :  DATA ARRAY.  EQUIVALENCED TO INTEGER, REAL, AND   
C              :  LOGICAL VARIABLES.    
C  DNANAM(K)   :  NAME ASSIGNED DATA BLOCK. 
C  DHACS       :  STRING FOR CHARACTER DATA.    
C   
C  **************  END DESCRIPTION OF /DHSDAT/  **************  
