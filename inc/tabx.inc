C  ---------COMMON,TABX :KEEPS TABLES FOR X RAY ENERGY DISTRIBUTIONS
      INTEGER NSTREF,NGAREF,NBINE
      REAL GSTEP
      PARAMETER(NSTREF=3,NGAREF=10,GSTEP=500.)
      COMMON/TABX/STREF(NSTREF),GAMREF(NGAREF),XNUMB(NSTREF,NGAREF),
     +            NBINE, PROBX(500,NSTREF,NGAREF)
C  STREF(I)     =REFERENCE SIN(THETA) IN BIN I(OF THETA)
C  GAMREF(J)    =REFERENCE GAMMA      IN BIN J(OF GAMMA)
C  XNUMB(I,J)   = NB. OF X RAYS FOR BIN I IN THETA,BIN J IN GAMMA
C  PROBX(I,J,K) = CORRESPONDING PROBABILITY DISTRIBUTION    
      REAL        STREF,GAMREF,XNUMB,PROBX
