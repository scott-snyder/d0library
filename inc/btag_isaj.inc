      COMMON /BTAG_ISAJ/ NTAG,JET_TAGGED(2),DIF_R(2),DIF_ET(2),
     &  NAME_TAG(2), TAG_YESNO(NJETS_MAX),BTAG_FL,BT(8),BB(8),
     &  BTL(9),BBL(9),LEPTON_TAGGED(2),DELTAL_BT(2),DELTAL_BB(2),
     &  TP(9),TB(9)
C
      INTEGER NTAG,JET_TAGGED,TAG_YESNO
      REAL    DIF_R,DIF_ET
      REAL    BT,BB
      REAL    BTL,BBL
      CHARACTER*2 NAME_TAG
      INTEGER BTAG_FL
      INTEGER LEPTON_TAGGED
      REAL    DELTAL_BT,DELTAL_BB
      REAL    TP,TB
C
C NTAG = NUMBNER OF TAGS
C JET_TAGGED(2) . NUMBER OF JET TAGGED AS EITHER A BT OR A BB
C THE NUMBER IS REFERENCE TO /EVENT_QUAN1/
C DIF_R = DIFFERENCE IN R BETWEEN PARENT PARTON JET AND OBSERVED JET
C DIF_ET = DIFFERENCE IN ET BETWEEN PARENT JET AND PARTON JET
C NAME_TAG(2) = TAGGED NAME ('BT' OR 'BB' ) OF JET.
C TAG_YESNO(NJETS_MAX) =0 IF JET IN /EVENT_QUAN1/ IS NOT TAGGED AS A BT OR BB
C                      =1 FOR BT
C                      =2 FOR BB
C
C BTAG_FL = BTAG FLAG FOR COMBINATION.
C BT,BB 8 VECTORS OF ISAQ TOP, TBAR BANKS
C BTL,BBL ISAL VECTORS OF LEPTONS THAT LINK TO BT,BB
C LEPTON_TAGGED(1)=1 IS THE TOP DECAYS TO LEPTON1S
C LEPTON_TAGGED(2)=1 IS THE TBAR DECAYS TO LEPTON1S AND VICE VERSA
C DELTAL_BT,DELTAL_BB ARE THE DIFFERNCES IN R BETWEEN LEPTONS 1 AND 2
C WITH TOP AND BOTTOM DECAY LEPTONS
C TP,TB ARE THE 9 VECTORS OF TOP AND TOP-BAR ISAJ QUARKS
